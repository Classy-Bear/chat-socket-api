{
	"env": {
		"commonjs": true,
		"es2021": true,
		"node": true,
		"jest/globals": true
	},
	"extends": [
		"airbnb-base",
		"plugin:jest/all"
	],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"plugins": [
		"jest"
	],
	"rules": {
		"padding-line-between-statements": [
			"error",
			{
				"blankLine": "always",
				"prev": "*",
				"next": "cjs-export"
			},
			{
				"blankLine": "always",
				"prev": "*",
				"next": "function"
			}
		]
	}
}
